<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajendra.onlineproductsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:exported="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Productdetails"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".CartActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".ChangePassActivity"></activity>
        <activity android:name=".PayActivity"></activity>
        <activity android:name=".AddressActivity"></activity>
        <activity android:name=".AddAddressActivity"></activity>
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>